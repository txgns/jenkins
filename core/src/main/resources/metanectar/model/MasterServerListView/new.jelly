<st:compress xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
             xmlns:t="/lib/hudson" xmlns:f="/lib/form" xmlns:i="jelly:fmt">
  <l:layout title="${%Add New Nectar/Jenkins}">
    <st:include page="sidepanel.jelly"/>
    <l:main-panel>
      <h1><img src="${imagesURL}/32x32/computer.png"/> ${%Add New Nectar/Jenkins}</h1>
      <p>
        Put your Nectar/Jenkins under the control of MetaNectar by adding it.
      </p>
      <f:form method="post" action="addNectar" name="addNectar">
        <f:entry title="${%URL}">
          <f:textbox name="url"/>
        </f:entry>
        <f:block>
          <f:submit value="${%Add}"/>
        </f:block>
      </f:form>
    </l:main-panel>
  </l:layout>
</st:compress>
